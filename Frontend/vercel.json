{
    "version": 2,
    "builds": [
      {
        "src": "package.json", 
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" } 
      }
    ],
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/index.html", 
        "status": 200
      },
      { 
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }